<div class="l-row">
  {{#each imgs}}
    <div class="l-item-1-4 c-image js-image" data-url="{{ urlImg }}">
      <img src="{{ urlThumb }}">
      <figcaption class="c-image__figcaption">
        <h2 class="c-image__description">{{ description }}
          <span class="c-image__icon">
            <svg fill="#FFFFFF" height="34" viewBox="0 0 24 24" width="34" xmlns="http://www.w3.org/2000/svg">
                <path d="M24 24H0V0h24v24z" fill="none"/>
                <path d="M21 15h2v2h-2v-2zm0-4h2v2h-2v-2zm2 8h-2v2c1 0 2-1 2-2zM13 3h2v2h-2V3zm8 4h2v2h-2V7zm0-4v2h2c0-1-1-2-2-2zM1 7h2v2H1V7zm16-4h2v2h-2V3zm0 16h2v2h-2v-2zM3 3C2 3 1 4 1 5h2V3zm6 0h2v2H9V3zM5 3h2v2H5V3zm-4 8v8c0 1.1.9 2 2 2h12V11H1zm2 8l2.5-3.21 1.79 2.15 2.5-3.22L13 19H3z"/>
            </svg>
          </span>

        </h2>
      </figcaption>
    </div>
  {{/each}}
  <p class="text-green"> Fotografie: <a class="link" href="https://unsplash.com/">unsplash.com</a></p>
</div>

<div id="js-modal" class="modal"></div>
<script id="js-modal-tmp" type="text/x-handlebars-template">
  <div class="modal__container">
    <img id="js-modal-img" class="c-modal__img" alt="" src="{{ url }}">
    <div class="modal__ctrls">
    </div>
  </div>
</script>
